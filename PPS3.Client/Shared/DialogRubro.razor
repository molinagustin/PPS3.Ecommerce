<MudDialog>
    <DialogContent>
        ¿Ir a agregar un nuevo tipo de producto del rubro creado?
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">No</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit">Si</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}